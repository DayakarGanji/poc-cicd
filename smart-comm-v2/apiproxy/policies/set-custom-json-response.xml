<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="set-custom-json-response">
    <DisplayName>set-custom-json-response</DisplayName>
    <Properties/>
    <Set>
        <Payload Content-Type="application/json">{substring(response.content,9)}</Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="response"/>
</AssignMessage>